{
  "name": "@mikister/gitsync",
  "version": "0.0.2",
  "author": "Milan Radojevic @mikister",
  "bin": {
    "gitsync": "./bin/run"
  },
  "bugs": "https://github.com/mikister/gitsync/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.24.1",
    "@types/node": "^10.17.47",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.15.0",
    "globby": "^10.0.2",
    "prettier": "^2.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mikister/gitsync",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gitsync",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mikister/gitsync",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "format": "prettier --write \"**/*.+(ts|js|jsx|json|css|md)\"",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts ."
  },
  "types": "lib/index.d.ts"
}
